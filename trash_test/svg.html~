	<script type="text/javascript" src="svg.js"></script>
	<script type="text/javascript">
	    var p = new svg.Paper("container");
	    var r1 = new svg.Rect();
	    p.addElement(r1);
	    setTimeout(function(){r1.node.setAttribute("width", "900"()}, 2000);
	</script>
        
